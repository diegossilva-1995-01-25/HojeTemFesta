package view;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;
import java.awt.BorderLayout;
import javax.swing.JPanel;
import javax.swing.JLabel;
import java.awt.GridLayout;
import javax.swing.JTextField;

public class PainelCadastro extends PainelGenerico implements ActionListener {

	private static final long serialVersionUID = -3531168108973346408L;
	private JTextField txtIdMembro;
	private JTextField textField;
	private JTextField textField_1;
	private JTextField textField_2;
	private JTextField textField_3;

	/**
	 * Create the panel.
	 */
	public PainelCadastro() {
		setLayout(new BorderLayout(0, 0));
		
		JPanel painelCampos = new JPanel();
		add(painelCampos, BorderLayout.NORTH);
		painelCampos.setLayout(new GridLayout(0, 2, 5, 5));
		
		JLabel lblIdMembro = new JLabel("Id do Membro");
		painelCampos.add(lblIdMembro);
		
		txtIdMembro = new JTextField();
		painelCampos.add(txtIdMembro);
		txtIdMembro.setColumns(10);
		
		JLabel label = new JLabel("Id do Membro");
		painelCampos.add(label);
		
		textField = new JTextField();
		textField.setColumns(10);
		painelCampos.add(textField);
		
		JLabel label_1 = new JLabel("Id do Membro");
		painelCampos.add(label_1);
		
		textField_1 = new JTextField();
		textField_1.setColumns(10);
		painelCampos.add(textField_1);
		
		JLabel label_2 = new JLabel("Id do Membro");
		painelCampos.add(label_2);
		
		textField_2 = new JTextField();
		textField_2.setColumns(10);
		painelCampos.add(textField_2);
		
		JLabel label_3 = new JLabel("Id do Membro");
		painelCampos.add(label_3);
		
		textField_3 = new JTextField();
		textField_3.setColumns(10);
		painelCampos.add(textField_3);

	}

	@Override
	public void painelParaMembro() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void membroParaPainel() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void actionPerformed(ActionEvent e) {
		// TODO Auto-generated method stub
		
	}

}
